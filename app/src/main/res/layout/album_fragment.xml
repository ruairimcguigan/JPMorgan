<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:baseInjectorApplication="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/album"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.album.AlbumFragment">


    <androidx.recyclerview.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="0dp"
            baseInjectorApplication:layout_constraintTop_toTopOf="parent"
            baseInjectorApplication:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="@dimen/grid_8"
            baseInjectorApplication:layout_constraintStart_toStartOf="parent" android:layout_marginStart="@dimen/grid_8"
            android:layout_marginBottom="@dimen/grid_8" baseInjectorApplication:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="@dimen/grid_8" android:id="@+id/album_list"/>
</androidx.constraintlayout.widget.ConstraintLayout>
