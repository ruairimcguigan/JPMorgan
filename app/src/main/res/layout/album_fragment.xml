<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:baseInjectorApplication="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:id="@+id/album"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".ui.album.AlbumFragment">


    <android.support.v7.widget.RecyclerView
            android:layout_width="0dp"
            android:layout_height="0dp"
            baseInjectorApplication:layout_constraintTop_toTopOf="parent"
            baseInjectorApplication:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            baseInjectorApplication:layout_constraintStart_toStartOf="parent" android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp" baseInjectorApplication:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginTop="8dp" android:id="@+id/album_list"/>
</android.support.constraint.ConstraintLayout>
